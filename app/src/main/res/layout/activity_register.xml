<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RegisterActivity">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="173dp"
        android:layout_height="140dp"
        android:contentDescription="icono_registrar_cuenta"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022"
        app:srcCompat="@drawable/user_plus_solid"
        app:tint="#DE84FF"
        tools:ignore="ImageContrastCheck" />

    <TextView
        android:id="@+id/tv_register"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/register"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.23" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.65">

        <TextView
            android:id="@+id/tv_CompleteName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="@string/completeName"
            android:textAlignment="center"
            android:textSize="16sp" />

        <EditText
            android:id="@+id/et_CompleteName"
            android:layout_width="300dp"
            android:layout_height="37dp"
            android:ems="10"
            android:hint="Nombre del NiÃ±o"
            android:inputType="textPersonName"
            android:textColorHint="#757575"
            android:textSize="14sp"
            tools:ignore="TouchTargetSizeCheck"
            android:autofillHints="" />

        <TextView
            android:id="@+id/tv_EmailAddress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="@string/Email"
            android:textSize="16sp" />

        <EditText
            android:id="@+id/et_EmailAddress"
            android:layout_width="300dp"
            android:layout_height="37dp"
            android:ems="10"
            android:hint="@string/Email"
            android:importantForAutofill="no"
            android:inputType="textEmailAddress"
            android:textColorHint="#757575"
            android:textSize="14sp"
            tools:ignore="TouchTargetSizeCheck,DuplicateSpeakableTextCheck" />

        <TextView
            android:id="@+id/tv_Password"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="@string/password"
            android:textSize="16sp" />

        <EditText
            android:id="@+id/et_Password"
            android:layout_width="300dp"
            android:layout_height="37dp"
            android:ems="10"
            android:hint="@string/password"
            android:importantForAutofill="no"
            android:inputType="textPassword"
            android:textColorHint="#757575"
            android:textSize="14sp"
            tools:ignore="TouchTargetSizeCheck,DuplicateSpeakableTextCheck" />

        <TextView
            android:id="@+id/tv_ConfirmPassword"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="@string/confirmPassword"
            android:textSize="16sp" />

        <EditText
            android:id="@+id/et_ConfirmPassword"
            android:layout_width="300dp"
            android:layout_height="37dp"
            android:ems="10"
            android:hint="@string/confirmPassword"
            android:inputType="textPassword"
            android:textColorHint="#757575"
            android:textSize="14sp"
            tools:ignore="DuplicateSpeakableTextCheck,TouchTargetSizeCheck"
            android:importantForAutofill="no" />

        <TextView
            android:id="@+id/tv_ControlParental"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="@string/control_parental"
            android:textSize="16sp" />

        <EditText
            android:id="@+id/et_ControlParental"
            android:layout_width="300dp"
            android:layout_height="37dp"
            android:ems="10"
            android:hint="Clave PIN"
            android:inputType="number"
            android:textSize="14sp"
            tools:ignore="TouchTargetSizeCheck"
            android:importantForAutofill="no" />

        <TextView
            android:id="@+id/tv_ConfirmarControlParental"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="Confirmar Control Parental"
            android:textSize="16sp" />

        <EditText
            android:id="@+id/et_ConfirmarControlParental"
            android:layout_width="300dp"
            android:layout_height="37dp"
            android:ems="10"
            android:hint="Confirmar Clave PIN"
            android:inputType="number"
            android:textColorHint="#757575"
            android:textSize="14sp"
            tools:ignore="TouchTargetSizeCheck"
            android:importantForAutofill="no" />

    </LinearLayout>

    <Button
        android:id="@+id/btn_Register"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/register"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.95"
        tools:ignore="DuplicateSpeakableTextCheck" />


</androidx.constraintlayout.widget.ConstraintLayout>